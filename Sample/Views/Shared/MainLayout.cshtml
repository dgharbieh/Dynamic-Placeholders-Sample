@using Sitecore.Mvc
@* @using Sitecore.Mvc.Analytics.Extensions *@
@{
    Layout = null;
}
<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
    <title>Financial Services</title>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <link rel="stylesheet" href="~/style/style.css" type="text/css" />
    <link rel="stylesheet" type="text/css" href="~/style/jquery-foxiblob-0.1.css" />
    <script type="text/javascript" src="~/js/jquery-1.3.2.min.js"></script>
    <script type="text/javascript" src="~/js/jquery-foxiblob-0.1.min.js"></script>
    <script type="text/javascript" src="~/js/jquery.corners.min.js"></script>
</head>
<body>
    <script type="text/javascript">$(document).ready(function () { $('#menu_list').foxiblob({ opacity: 0.6 }); });</script>
    <script>$(document).ready(function () { $('.rounded').corners("10px"); });</script>
    <div id="wrapper">
  
        @Html.Sitecore().Placeholder("header")
        <!--end header-->
        <!--end head-->

        @Html.Sitecore().Placeholder("banner")
     
        <div id="container">
            <div id="top">

                @Html.Sitecore().Placeholder("main")
             
               
  
              
                </div>
            <div id="bottom">
                @Html.Sitecore().Placeholder("bottom")
                </div>

                <!--end bottom-->
            </div>
        <!--end container-->
        @Html.Sitecore().Placeholder("footer")
        <!--end footer-->
    </div>
    <!--end wrapper-->


</body>
</html>
